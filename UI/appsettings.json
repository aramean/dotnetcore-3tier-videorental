  "DatabaseConnectionStrings": {
    "SQLServer": {
      "DefaultConnection": "Server=localhost;Database=MyDb;Trusted_Connection=True;"
    },
    "InMemory": {
      "DefaultConnection": "databaseName: MyDb"
    }
