{
  "DatabaseConnectionStrings": {
    "InMemory": {
      "DefaultConnection": "databaseName: MyDb"
    },
    "SQLServer": {
      "DefaultConnection": "Server=localhost;Database=MyDb;Trusted_Connection=True;"
    },
    "PostgreSQL": {
      "DefaultConnection": "Server=localhost;Port=5432;Database=MyDb;User Id=postgres;Password=postgres;"
    },
    "MySQL": {
      "DefaultConnection": "Server=localhost;Database=myDb;User=root;Password=mysql;"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "DefaultDatabaseProvider": "InMemory",
  "AllowedHosts": "*"
}
